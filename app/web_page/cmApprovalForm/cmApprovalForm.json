    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cmApprovalForm && $data.cmApprovalForm_poplanning ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.cmApprovalForm.poplanning = $data.cmApprovalForm_poplanning;",
        "\treturn {",
        "\t\t//map cmApprovalForm variable to expected task contract input",
        "\t\tcmApprovalFormInput: {",
        "\t\t\tpoplanning: $data.cmApprovalForm.poplanning ? {",
        "\t\t\t\tpersistenceId_string: $data.cmApprovalForm.poplanning.persistenceId_string !== undefined ? $data.cmApprovalForm.poplanning.persistenceId_string : null,",
        "\t\t\t\tassignTo: $data.cmApprovalForm.poplanning.assignTo !== undefined ? $data.cmApprovalForm.poplanning.assignTo : null,",
        "\t\t\t\taddRefStore: $data.cmApprovalForm.poplanning.addRefStore !== undefined ? $data.cmApprovalForm.poplanning.addRefStore : null,",
        "\t\t\t\taddPriorityStoreName: $data.cmApprovalForm.poplanning.addPriorityStoreName !== undefined ? $data.cmApprovalForm.poplanning.addPriorityStoreName : null",
        "\t\t\t} : null,",
        "\t\t\tapproval: $data.cmApprovalForm.approval !== undefined ? $data.cmApprovalForm.approval : null",
        "\t\t}",
        "\t}",
        "}"
      ],
