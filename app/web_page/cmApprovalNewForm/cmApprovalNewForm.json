    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "cmApproval_poplanning" : {
      "type" : "url",
      "value" : [
        "{{cmApproval|lazyRef:'poplanning'}}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cmApproval && $data.cmApproval_poplanning ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.cmApproval.poplanning = $data.cmApproval_poplanning;",
        "\treturn {",
        "\t\t//map cmApproval variable to expected task contract input",
        "\t\tcmApprovalInput: {",
        "\t\t\tpoplanning: $data.cmApproval.poplanning ? {",
        "\t\t\t\tpersistenceId_string: $data.cmApproval.poplanning.persistenceId_string !== undefined ? $data.cmApproval.poplanning.persistenceId_string : null,",
        "\t\t\t\tassignTo: $data.cmApproval.poplanning.assignTo !== undefined ? $data.cmApproval.poplanning.assignTo : null,",
        "\t\t\t\taddRefStore: $data.cmApproval.poplanning.addRefStore !== undefined ? $data.cmApproval.poplanning.addRefStore : null,",
        "\t\t\t\taddPriorityStoreName: $data.cmApproval.poplanning.addPriorityStoreName !== undefined ? $data.cmApproval.poplanning.addPriorityStoreName : null",
        "\t\t\t} : null,",
        "\t\t\tapproval: $data.cmApproval.approval !== undefined ? $data.cmApproval.approval : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
