    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.branchDetails ){",
        "\treturn {",
        "\t\t//map branchDetails variable to expected task contract input",
        "\t\tbranchDetailsInput: {",
        "\t\t\tbranchName: $data.branchDetails.branchName !== undefined ? $data.branchDetails.branchName : null,",
        "\t\t\tbranchCode: $data.branchDetails.branchCode !== undefined ? $data.branchDetails.branchCode : null,",
        "\t\t\taddress: $data.branchDetails.address !== undefined ? $data.branchDetails.address : null,",
        "\t\t\tpincode: $data.branchDetails.pincode !== undefined ? $data.branchDetails.pincode : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
